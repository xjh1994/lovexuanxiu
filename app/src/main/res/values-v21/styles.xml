<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="AppStart" parent="MaterialNavigationDrawerTheme.Light.DarkActionBar.TranslucentKitKatStatusBar">
        <item name="android:windowBackground">@drawable/start</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowFullscreen">true</item>
        <item name="android:navigationBarColor">@color/black</item>
    </style>

    <style name="MyNavigationDrawerTheme" parent="MaterialNavigationDrawerTheme.Light.DarkActionBar.TranslucentKitKatStatusBar">
        <item name="colorPrimary">@color/primarycolor</item>
        <item name="colorPrimaryDark">@color/darkprimarycolor</item>
        <item name="colorAccent">#03A9F4</item>
        <item name="learningPattern">false</item>
        <item name="singleAccount">true</item>
        <item name="android:textColor">@color/primarytextcolor</item>
        <item name="rippleBackport">true</item>

        <item name="android:actionMenuTextColor">@color/white</item>
        <item name="actionMenuTextColor">@color/white</item>
        <item name="android:navigationBarColor">@android:color/white</item>
        <item name="android:colorForeground">@android:color/white</item>
        <item name="android:textColorPrimary">@android:color/white</item>
    </style>
</resources>